{
  "name": "react-app",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "watch:development1": "CONFIG=development1 watchify -d -v -t babelify ./src/app.js -o ./dist/build.js",
    "watch:development2": "CONFIG=development2 watchify -d -v -t babelify ./src/app.js -o ./dist/build.js",
    "watch:staging": "CONFIG=staging watchify -d -v -t babelify ./src/app.js -o ./dist/build.js",
    "watch:production": "CONFIG=production watchify -d -v -t babelify ./src/app.js -o ./dist/build.js",
    "serve": "browser-sync start --server --files ./dist/build.js",
    "lint": "eslint ./src/* ./test/*; exit 0",
    "flow": "flow check --color always; exit 0",
    "test": "CONFIG=spec mocha -r babel-register -r jsdom-global/register -R spec -c ./test/**/*Spec.js; exit 0",
    "test:min": "CONFIG=spec mocha -r babel-register -r jsdom-global/register -R min -c ./test/**/*Spec.js; exit 0",
    "lint-flow-test": "npm-run-all lint flow test:min",
    "build": "NODE_ENV=production CONFIG=production browserify -t babelify ./src/app.js | uglifyjs -mc -o ./dist/build.js"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.13.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-enzyme": "^0.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-flowtype": "^2.18.2",
    "eslint-plugin-import": "^1.11.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "flow-bin": "^0.32.0",
    "jsdom": "^9.4.1",
    "jsdom-global": "^2.1.0",
    "mocha": "^2.5.3",
    "npm-run-all": "^3.1.0",
    "react-addons-test-utils": "^15.3.1",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0",
    "uglify-js": "^2.7.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "react": "^15.3.1",
    "react-dispatcher-decorator": "^0.1.5",
    "react-dom": "^15.3.1",
    "react-router": "^2.8.1"
  }
}
